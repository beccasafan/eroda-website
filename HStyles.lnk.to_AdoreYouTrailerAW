            !function(e,n,u,a){e.twq||(a=e.twq=function(){a.exe?a.exe.apply(a,arguments):
            a.queue.push(arguments);},a.version='1',a.queue=[],t=n.createElement(u),
            t.async=!0,t.src='//static.ads-twitter.com/uwt.js',s=n.getElementsByTagName(u)[0],
            s.parentNode.insertBefore(t,s))}(window,document,'script');
            triggerTwitterUniversal('pageview');
        }
    }
    var twitterUniversalIds = ['o2jwo'];
    function triggerTwitterUniversal(event){
        for(var i = 0; i < twitterUniversalIds.length; i++){
            twq('init',twitterUniversalIds[i]);
            twq('track',event);
        }
    }
</script>
<script>
    window.linkfire.retargetingPixels['googleglobal'].loader = function() {
        if (!checkScriptLoaded('advertiser', 'googleglobal')) {
            var googlescript = document.createElement('script');
            googlescript.src='https://www.googletagmanager.com/gtag/js?id=AW-757474210';
            googlescript.type='text/javascript';
            googlescript.onload = function() {
                window.googleTagIds = [{"id":"AW-757474210","label":"\/d-MECKTpo6cBEKLHmOkC"},{"id":"AW-757474210","label":"\/rfK4CMjpo6cBEKLHmOkC"},{"id":"AW-716855806","label":null}];
                window.googleTagData = window.linkfire.retargetingPixels.googleglobal.tags;
                window.dataLayer = window.dataLayer || [];
                gtag('js', new Date());
                                gtag('config', 'AW-757474210');
                                gtag('config', 'AW-757474210');
                                gtag('config', 'AW-716855806');
                                triggerGlobalTags('page_view');
            }
            document.getElementsByTagName('Body').item(0).appendChild(googlescript);

        }
    }
    function gtag() {
        dataLayer.push(arguments)
    }

    function triggerGlobalTags(event){
        for(var i = 0; i < window.googleTagIds.length; i++){
        var data = JSON.parse(JSON.stringify(window.googleTagData));
        data.send_to = window.googleTagIds[i].id + (window.googleTagIds[i].label ? window.googleTagIds[i].label : '');
        gtag('event', event, data);
        }
    }
</script>
