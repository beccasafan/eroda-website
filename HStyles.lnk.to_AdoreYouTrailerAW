<script>
    window.linkfire.retargetingPixels['facebook'].loader = function() {
        if (!checkScriptLoaded('advertiser','facebook')) {
            !function(f,b,e,v,n,t,s){if(f.fbq)return;n=f.fbq=function(){n.callMethod?
            n.callMethod.apply(n,arguments):n.queue.push(arguments)};if(!f._fbq)f._fbq=n;
            n.push=n;n.loaded=!0;n.version='2.0';n.queue=[];t=b.createElement(e);t.async=!0;
            t.src=v;s=b.getElementsByTagName(e)[0];s.parentNode.insertBefore(t,s)}(window,
            document,'script','//connect.facebook.net/en_US/fbevents.js');

            window.linkfire.retargetingPixels.facebook.ids.forEach(function(id) {
                if (id === "399736106876066") {
                    fbq('init', id , {external_id: window.linkfire.settings.visitorToken});
                } else {
                    fbq('init', id);
                }
            });
            window._fbdata = window.linkfire.retargetingPixels.facebook.tags;
            if(window.linkfire.player != null) {
                window._fbdata.action = 'service';
                window._fbdata.musicservice = window.linkfire.player;
            }
            if(window.linkfire.appClickPlayer != null) {
                window._fbdata.eventlabel = window.linkfire.appClickPlayer;
            }
            fbq('trackCustom', 'LF', window._fbdata);
            fbq('track', 'PageView');
        }
    }
</script>

    <script>
        var google_custom_params = window.linkfire.retargetingPixels.google.tags;
        var conv_handler = null;
        var gaData = window.linkfire.retargetingPixels.google.gaData;
        function TriggerGooglePixel(){
            for(var i = 0; i < gaData.length; i++){
                window.google_conversion_id = gaData[i].id;
                window.google_conversion_label = gaData[i].label;
                window.google_remarketing_only = true;
                if (typeof(conv_handler) == 'function') {
                    conv_handler();
                }
            }
        }
        window.linkfire.retargetingPixels['google'].loader = function() {
            if (!checkScriptLoaded('advertiser','google')) {
                var googlescript = document.createElement('script');
                googlescript.src='//www.googleadservices.com/pagead/conversion_async.js';
                googlescript.type='text/javascript';
                googlescript.onload = function() {
                    conv_handler = window['google_trackConversion'];
                    TriggerGooglePixel();
                }
                document.getElementsByTagName('Body').item(0).appendChild(googlescript);
            }
        }
    </script>
<script>
    var snapIds = ["af0f7b9c-4a8f-49a3-a1b8-4c9fab88e3fc"];

    window.linkfire.retargetingPixels['snap'].loader = function() {
        if (!checkScriptLoaded('advertiser','snap')) {
            (function(win, doc, sdk_url){
            if(win.snaptr) return;
            var tr=win.snaptr=function(){
                tr.handleRequest? tr.handleRequest.apply(tr, arguments):tr.queue.push(arguments);
            };
            tr.queue = [];
            var s='script';
            var new_script_section=doc.createElement(s);
            new_script_section.async=!0;
            new_script_section.src=sdk_url;
            var insert_pos=doc.getElementsByTagName(s)[0];
            insert_pos.parentNode.insertBefore(new_script_section, insert_pos);
            })(window, document, 'https://sc-static.net/scevent.min.js');

            snapIds.forEach(function(id) {
                snaptr('init', id);
            });

            triggerSnap('pageview');
        }
    }

    function triggerSnap(eventType) {
        if (typeof(snaptr) === 'undefined' || typeof(snaptr) !== 'function') {
            return;
        }

        var snapEvents = {
          'player': 'VIEW_CONTENT',
          'service': 'START_CHECKOUT',
          'pageview': 'PAGE_VIEW'
        };

        snaptr('track', snapEvents[eventType]);
    }
</script>
    <script>
        window.linkfire.retargetingPixels['twitter'].loader = function() {
            if (!checkScriptLoaded('advertiser','twitter')) {
                var twitterscript = document.createElement('script');
                twitterscript.src='//platform.twitter.com/oct.js';
                twitterscript.type='text/javascript';
                twitterscript.onload = function() {
                    triggerTwitterIds(ids);
                }
                document.getElementsByTagName('Body').item(0).appendChild(twitterscript);
            }
        }
        function triggerTwitterIds(ids){
            if(window.twttr) {
                for(var i = 0; i < ids.length; i++){
                    twttr.conversion.trackPid(ids[i], { tw_sale_amount: 0, tw_order_quantity: 0 });
                }
			}
        }
        var ids = ['numgi'];
    </script>
<script>
    window.linkfire.retargetingPixels['twitteruniversal'].loader = function() {
